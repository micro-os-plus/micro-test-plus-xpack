      "clean-all": [
        "xpm run clean-native-cmake",
        "xpm run clean-native-meson",
        "xpm run clean-cortex-cmake",
        "xpm run clean-cortex-meson"
      ],
      "test": [
        "xpm run test-native-cmake-gcc",
        "xpm run test-native-meson-clang",
        "xpm run test-qemu-cortex-m4f-cmake",
        "xpm run test-qemu-cortex-m4f-meson"
