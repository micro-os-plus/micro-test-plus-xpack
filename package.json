{
  "name": "@micro-os-plus/micro-test-plus",
  "version": "2.0.0",
  "description": "A source xPack with ÂµTest++, a minimalistic testing infrastructure",
  "main": "",
  "scripts": {
    "comment_": "echo ctest -V shows test output; cmake --verbose shows the build commands",
    "link-deps": "xpm link @micro-os-plus/build-helper @micro-os-plus/platform-stm32f4discovery @micro-os-plus/devices-stm32f4-extras @micro-os-plus/architecture-cortexm @micro-os-plus/startup @micro-os-plus/semihosting @micro-os-plus/diag-trace @micro-os-plus/libs-c @micro-os-plus/libs-cpp @micro-os-plus/version",
    "rm-deps": "xpm uninstall --global @micro-os-plus/build-helper @micro-os-plus/platform-stm32f4discovery @micro-os-plus/devices-stm32f4-extras @micro-os-plus/architecture-cortexm @micro-os-plus/startup @micro-os-plus/semihosting @micro-os-plus/diag-trace @micro-os-plus/libs-c @micro-os-plus/libs-cpp @micro-os-plus/version",
    "build-tests-native-cmake-debug": "cmake -S tests/meta -B build/tests-native-cmake-debug -GNinja -DPLATFORM_NAME=native -DCMAKE_BUILD_TYPE=Debug && cmake --build build/tests-native-cmake-debug",
    "build-tests-native-cmake-debug-verbose": "cmake -S tests/meta -B build/tests-native-cmake-debug -GNinja -DPLATFORM_NAME=native -DCMAKE_BUILD_TYPE=Debug && cmake --build build/tests-native-cmake-debug --verbose",
    "clean-tests-native-cmake-debug": "cmake --build build/tests-native-cmake-debug --verbose --target clean",
    "execute-tests-native-cmake-debug": "cd build/tests-native-cmake-debug && ctest -V",
    "test-native-cmake-debug": "xpm run build-tests-native-cmake-debug && xpm run execute-tests-native-cmake-debug",
    "build-tests-native-cmake-release": "cmake -S tests/meta -B build/tests-native-cmake-release -GNinja -DPLATFORM_NAME=native -DCMAKE_BUILD_TYPE=release && cmake --build build/tests-native-cmake-release",
    "build-tests-native-cmake-release-verbose": "cmake -S tests/meta -B build/tests-native-cmake-release -GNinja -DPLATFORM_NAME=native -DCMAKE_BUILD_TYPE=release && cmake --build build/tests-native-cmake-release --verbose",
    "clean-tests-native-cmake-release": "cmake --build build/tests-native-cmake-release --verbose --target clean",
    "execute-tests-native-cmake-release": "cd build/tests-native-cmake-release && ctest -V",
    "test-native-cmake-release": "xpm run build-tests-native-cmake-release && xpm run execute-tests-native-cmake-release",
    "build-tests-stm32f4discovery-cmake-debug": "cmake -S tests/meta -B build/tests-stm32f4discovery-cmake-debug -GNinja -DCMAKE_TOOLCHAIN_FILE=../../xpacks/micro-os-plus-build-helper/cmake/toolchain-arm-none-eabi-gcc.cmake -DPLATFORM_NAME=stm32f4discovery -DCMAKE_BUILD_TYPE=Debug && cmake --build build/tests-stm32f4discovery-cmake-debug",
    "build-tests-stm32f4discovery-cmake-debug-verbose": "cmake -S tests/meta -B build/tests-stm32f4discovery-cmake-debug -GNinja -DCMAKE_TOOLCHAIN_FILE=../../xpacks/micro-os-plus-build-helper/cmake/toolchain-arm-none-eabi-gcc.cmake -DPLATFORM_NAME=stm32f4discovery -DCMAKE_BUILD_TYPE=Debug && cmake --build build/tests-stm32f4discovery-cmake-debug --verbose",
    "clean-tests-stm32f4discovery-cmake-debug": "cmake --build build/tests-stm32f4discovery-cmake-debug --verbose --target clean",
    "execute-tests-stm32f4discovery-cmake-debug": "cd build/tests-stm32f4discovery-cmake-debug && ctest -V",
    "test-stm32f4discovery-cmake-debug": "xpm run build-tests-stm32f4discovery-cmake-debug && xpm run execute-tests-stm32f4discovery-cmake-debug",
    "build-tests-stm32f4discovery-cmake-release": "cmake -S tests/meta -B build/tests-stm32f4discovery-cmake-release -GNinja -DCMAKE_TOOLCHAIN_FILE=../../xpacks/micro-os-plus-build-helper/cmake/toolchain-arm-none-eabi-gcc.cmake -DPLATFORM_NAME=stm32f4discovery -DCMAKE_BUILD_TYPE=MinSizeRel && cmake --build build/tests-stm32f4discovery-cmake-release",
    "build-tests-stm32f4discovery-cmake-release-verbose": "cmake -S tests/meta -B build/tests-stm32f4discovery-cmake-release -GNinja -DCMAKE_TOOLCHAIN_FILE=../../xpacks/micro-os-plus-build-helper/cmake/toolchain-arm-none-eabi-gcc.cmake -DPLATFORM_NAME=stm32f4discovery -DCMAKE_BUILD_TYPE=MinSizeRel && cmake --build build/tests-stm32f4discovery-cmake-release --verbose",
    "clean-tests-stm32f4discovery-cmake-release": "cmake --build build/tests-stm32f4discovery-cmake-release --verbose --target clean",
    "execute-tests-stm32f4discovery-cmake-release": "cd build/tests-stm32f4discovery-cmake-release && ctest -V",
    "test-stm32f4discovery-cmake-release": "xpm run build-tests-stm32f4discovery-cmake-release && xpm run execute-tests-stm32f4discovery-cmake-release",
    "clean-tests-native-cmake": "xpm run clean-tests-native-cmake-debug && xpm run clean-tests-native-cmake-release",
    "clean-tests-stm32f4discovery-cmake": "xpm run clean-tests-stm32f4discovery-cmake-debug && xpm run clean-tests-stm32f4discovery-cmake-release",
    "clean-tests-cmake": "xpm run clean-tests-native-cmake-debug && xpm run clean-stm32f4discovery-native-cmake",
    "clean-tests": "xpm run clean-tests-cmake",
    "test-native-cmake": "xpm run test-native-cmake-debug && xpm run test-native-cmake-release",
    "test-stm32f4discovery-cmake": "xpm run test-stm32f4discovery-cmake-debug && xpm run test-stm32f4discovery-cmake-release",
    "test-cmake": "xpm run test-native-cmake && xpm run test-stm32f4discovery-cmake",
    "test": "xpm run test-cmake"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micro-os-plus/micro-test-plus-xpack.git/"
  },
  "bugs": {
    "url": "https://github.com/micro-os-plus/micro-test-plus-xpack/issues/"
  },
  "homepage": "https://github.com/micro-os-plus/micro-test-plus-xpack/",
  "keywords": [
    "xpack",
    "c++",
    "testing",
    "framework",
    "micro-os-plus"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul/"
  },
  "license": "MIT",
  "config": {},
  "dependencies": {},
  "1devDependencies": {
    "@xpack-dev-tools/cmake": "3.19.2-1.1",
    "@xpack-dev-tools/ninja-build": "1.10.1-1.1",
    "@xpack-dev-tools/qemu-arm": "2.8.0-12.1",
    "@xpack-dev-tools/arm-none-eabi-gcc": "10.2.1-1.1.2"
  },
  "devDependencies": {    
    "@micro-os-plus/build-helper": "github:micro-os-plus/build-helper-xpack#semver:2.0.0",
    "@micro-os-plus/platform-stm32f4discovery": "github:micro-os-plus/platform-stm32f4discovery-xpack#semver:2.0.0",
    "@micro-os-plus/devices-stm32f4-extras": "github:micro-os-plus/devices-stm32f4-extras-xpack#semver:2.0.0"
  },
  "2devDependencies": {    
    "@micro-os-plus/devices-stm32f4-extras": "github:micro-os-plus/devices-stm32f4-extras-xpack#semver:2.0.0",
    "@micro-os-plus/architecture-cortexm": "github:micro-os-plus/architecture-cortexm-xpack#semver:2.0.0",
    "@micro-os-plus/startup": "github:micro-os-plus/startup-xpack#semver:2.0.0",
    "@micro-os-plus/semihosting": "github:micro-os-plus/semihosting-xpack#semver:3.0.0",
    "@micro-os-plus/diag-trace": "github:micro-os-plus/diag-trace-xpack#semver:2.0.0",
    "@micro-os-plus/libs-c": "github:micro-os-plus/libs-c-xpack#semver:2.0.0",
    "@micro-os-plus/libs-cpp": "github:micro-os-plus/libs-cpp-xpack#semver:2.0.0",
    "@micro-os-plus/version": "github:micro-os-plus/version-xpack#semver:7.0.2"
  },
  "xpack": {}
}
